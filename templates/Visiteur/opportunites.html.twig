        }

        .button {
            font-size: 18px;
            line-height: 28px;
            padding: 0 8px;
            float: right;
            margin: 26px 15px;

            -webkit-border-radius: 3px;
            -moz-border-radius: 3px;
            border-radius: 3px;

            -webkit-box-shadow: 1px 1px 3px rgba(0,0,0,0.12);
            -moz-box-shadow: 1px 1px 3px rgba(0,0,0,0.12);
            box-shadow: 1px 1px 3px rgba(0,0,0,0.12);
        }

        .ok {
            color: #3ac569;
            border: 1px solid #3ac569;
        }

        .ok:hover {
            color: #fff;
            background-color: #3ac569;
            border: 1px solid #3ac569;
        }

        .no {
            color: #f1404b;
            border: 1px solid #f1404b;
        }

        .no:hover {
            color: #fff;
            background-color: #f1404b;
            border: 1px solid #f1404b;
        }
    </style>
    <div class="container">
        <div class="row">

            {% for o in opportunites | filter(opportunites => opportunites.isValid) |sort((a, b) => a.dateLimite <=> b.dateLimite)  %}


                    <div style="margin: 10px">

                        <hr>

                        <div  class="profile-card-4 text-center"><img height="100px"  src=" {{ asset('uploads/images/'  ~ o.image) }}" class="img img-responsive" >
                            <div style="background-color: lightblue"  class="profile-content">
                                <div style="font-size: 20px" >{{o.titre}}
                                    {#                            <p>{{o.association.titre}}</p>#}
                                </div>
                                <div class="profile-description">
                                    {{ o.description }}</div>
                                <span style="color:red; font-size: 12px" class="blog-date">    <i class="fa fa-bell" aria-hidden="true"></i>  Date Limite :                  {{ o.dateLimite | date('d/m/Y ') }}
                                </span>

                                                                <p style=" color:#0c5460; font-family: Lobster;">Lancé par :<a
                                                                            href="{{ path('UserAssoci',{"id":o.association.id}) }}"> {{ o.association.titre }}</a>
                                                                </p>
                                                                <p style=" color:#0c5460; font-family: Lobster;">Domaine concerné
                                                                    :{{ o.domaineConcerne }}</p>
                                                                <p style=" color:#0c5460; font-family: Lobster;">Region concerné par cette opportunité
                                                                    : <i class="fa fa-map-marker "></i> {{ o.region }}</p>

                                                                <p style=" color:#0c5460; font-family: Lobster;">Type d'offre :{{ o.typeOffre }}</p>



                                <div class="row">

                                    <a class="btn btn-success center-block " style="height: 30px" href="{{ o.lienFormPostul }}">
                                        postuler</a>

                                </div>
                            </div>

                        </div>
                    </div>
    {% endfor %}

        </div></div>
{% endblock %}